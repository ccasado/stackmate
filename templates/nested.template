      }
   },
   "Resources":{
      "NSVPXStack":{
         "Type":"AWS::CloudFormation::Stack",
         "Properties":{
            "TemplateURL":{
               "Ref":"NSCloudFormationURL"
            },
            "Parameters":{
               "VPXPrimary":"m1.large",
               "TenancyType":"default",
               "VpcID":"99",
               "NsIP":{
                  "Fn::Join":[
                     ".",
                     [
                        {
                           "Ref":"Octet1VPC"
                        },
                        {
                           "Ref":"Octet2VPC"
                        },
                        {
                           "Ref":"PrivateSubnetOct"
                        },
                        "100"
                     ]
                  ]
               },
               "ClientIP":{
                  "Fn::Join":[
                     ".",
                     [
                        {
                           "Ref":"Octet1VPC"
                        },
                        {
                           "Ref":"Octet2VPC"
                        },
                        {
                           "Ref":"PublicSubnetOct"
                        },
                        "175"
                     ]
                  ]
               }
            }
         }
      }
   },
   "Outputs":{
      "InstallServer":{
         "Description":"IP address of the Install Server (Starburst). RDP to this IP address as Administrator (Citrix123) from the Bastion host and run the AWS-Farm-Build.ps1 PowerShell script to finish installation.",
         "Value":"Blah"
      }
   }
}